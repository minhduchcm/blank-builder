{
  "name": "blank-builder",
  "version": "0.1.0",
  "repository": "https://github.com/minhduchcm/blank-builder.git",
  "author": "Lê Minh Đức",
  "license": "MIT",
  "engines": {
    "node": "8.1.4"
  },
  "scripts": {
    "start": "node build/server/index.js",
    "build": "yarn buildServer && yarn buildClient",
    "buildServer": "cross-env NODE_ENV=production babel src/server -d build/server --presets es2015,es2017,stage-0",
    "buildClient": "cross-env NODE_ENV=production webpack --config webpack/webpack.client.config.js",
    "dev": "concurrently -k \"npm run devServer\" \"npm run devClient\"",
    "devClient": "webpack-dev-server --config webpack/webpack.client.config.js",
    "devServer": "nodemon --watch src/server src/server/index.js --exec babel-node --presets es2015,es2017,stage-0",
    "prebuild": "rimraf build/*",
    "lint": "eslint src"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.5.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-prettier": "^2.2.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "postcss-cssnext": "^3.0.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.5.3",
    "react-transform-style": "^1.0.4",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^2.5.1",
    "webpack-dashboard": "^0.4.0",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-core": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-register": "^6.26.0",
    "body-parser": "^1.17.2",
    "bootstrap": "4.0.0-alpha.6",
    "classnames": "^2.2.5",
    "compression": "^1.7.0",
    "cross-env": "^5.0.1",
    "draft-js": "^0.10.1",
    "draft-js-export-html": "^0.6.0",
    "express": "^4.15.4",
    "html-to-react": "^1.2.11",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "morgan": "^1.8.2",
    "react": "^15.5.4",
    "react-addons-transition-group": "^15.5.2",
    "react-dom": "^15.5.4",
    "react-hot-loader": "next",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "react-tooltip": "^3.3.0",
    "react-transition-group": "^2.1.0",
    "redux": "^3.6.0",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0",
    "uuid": "^3.1.0"
  }
}
